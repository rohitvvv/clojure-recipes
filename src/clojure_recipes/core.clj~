
(ns clojure-recipes.core
  (:require [clojure-recipes.core :refer :all])
  (:gen-class))

(defn printNumbers
  "Print numbers less than x"
  ([] (printNumbers 0))
  ([x](println x)
   (if (< x 10)
     (printNumbers (inc x))
     (println "Done!"))))


(defn -main
  "I don't do a whole lot ... yet."
  [& args]
  (println "Hello, World!")
;  (printNumbers)
  )
